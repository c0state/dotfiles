[include]
	path = .gitconfig-custom
[user]
    name = c0state
    email = stephen.liu@alumni.stanford.edu
[color]
    ui = true
[format]
    pretty = oneline
[core]
    editor = vim
    filemode = false
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    switch = !legit switch \"$@\"
    branches = !legit branches
    sprout = !legit sprout \"$@\"
    unpublish = !legit unpublish \"$@\"
    harvest = !legit harvest \"$@\"
    sync = !legit sync \"$@\"
    publish = !legit publish \"$@\"
    graft = !legit graft \"$@\"
    up = !git pull --rebase --prune $@ && git submodule update --init --recursive
[difftool]
    prompt = false
[mergetool]
    prompt = false
